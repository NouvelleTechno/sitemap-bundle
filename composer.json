{
    "name": "nouvelletechno/sitemap-bundle",
    "description": "This bundle helps you generating a sitemap.xml file for your Symfony application",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.2.5",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/twig-bundle": "^4.3 || ^5.0",
        "symfony/twig-bridge": "^4.3 || ^5.0",
        "symfony/yaml": "^4.3 || ^5.0"
    },
    "license": "MIT",
    "keywords": [
        "xml",
        "seo",
        "sitemap.xml",
        "sitemap",
        "spider",
        "symfony",
        "bundle"
    ],
    "authors": [
        {
            "name": "Nouvelle-Techno.fr",
            "email": "contact@nouvelle-techno.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "NouvelleTechno\\SitemapBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NouvelleTechno\\SitemapBundle\\Tests\\": "tests/"
        }
    }
}
